  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "typeorm:generate": "migrationName=${npm_config_name:-migration} && typeorm migration:generate ./src/database/migrations/$migrationName -d ./src/database/datasource.js -o",
